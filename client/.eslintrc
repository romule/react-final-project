{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/jsx-filename-extension": [
            0,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
    }
}